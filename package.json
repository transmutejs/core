{
  "name": "transmutejs",
  "version": "8.0.6",
  "description": "Transmute, to change or alter in form, appearance, or nature and especially to a higher form",
  "main": "./index.js",
  "scripts": {
    "test": "ENV=test nyc --reporter=html --reporter=text mocha test/**.spec.js",
    "coverage": "ENV=test nyc report --reporter=text-lcov npm test | coveralls",
    "preversion": "npm test",
    "preversion:readme": "replace '/v\\d+\\.\\d+\\.\\d+' \"/v$npm_package_version\" ./README.md",
    "version": "npm run preversion:readme && git add -A README.md",
    "postversion": "git push && git push --tags && npm publish"
  },
  "bin": {
    "transmute": "./index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/transmutejs/core.git"
  },
  "keywords": [
    "transcode",
    "media",
    "h265",
    "h264",
    "ffmpeg"
  ],
  "author": "Jamie Holdroyd <jamie.holdroyd@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/transmutejs/core/issues"
  },
  "homepage": "https://github.com/transmutejs/core#readme",
  "files": [
    "LICENSE",
    "README.md",
    "index.js",
    "locales/",
    "app/"
  ],
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.1",
    "grunt": "^1.0.2",
    "grunt-contrib-jshint": "^1.1.0",
    "grunt-contrib-watch": "^1.0.1",
    "grunt-run": "^0.8.1",
    "istanbul": "^0.4.5",
    "mocha": "^5.1.1",
    "nyc": "^11.7.3",
    "replace": "^1.0.0",
    "sinon": "^5.0.7",
    "sinon-chai": "^3.0.0"
  },
  "dependencies": {
    "ajv": "^6.5.0",
    "chalk": "^2.4.1",
    "dashdash": "^1.14.1",
    "deep-assign": "^2.0.0",
    "dotenv": "^5.0.1",
    "filesize": "^3.6.1",
    "fluent-ffmpeg": "^2.1.2",
    "glob": "^7.1.2",
    "gpu-info": "0.0.1",
    "hasbin": "^1.2.3",
    "i18n": "^0.8.3",
    "inquirer": "^5.2.0",
    "moment": "^2.22.1",
    "moment-duration-format": "^2.2.2",
    "moviedb": "^0.2.10",
    "mv": "^2.1.1",
    "node-watch": "^0.5.8",
    "opn": "^5.3.0",
    "parse-torrent-name": "^0.5.4",
    "progress": "github:jHoldroyd/node-progress#fix/ansi-styling",
    "promise-queue": "^2.2.5",
    "restify": "^7.1.1",
    "socket.io": "^2.5.0",
    "strip-ansi": "^4.0.0",
    "tvmaze-node": "^1.0.4",
    "winston": "^2.4.2",
    "winston-daily-rotate-file": "^3.1.4"
  }
}
